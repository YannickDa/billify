{
  "name": "billify",
  "version": "1.0.0",
  "description": "Generate bills automaticly",
  "main": "index.js",
  "scripts": {
    "dev": "run-p development-build-server development-prepare-server-build development-server-delayed",
    "development-prepare-server-build": "BABEL_ENV=server-dev universal-webpack --settings ./webpack/universal-webpack-settings.json prepare",
    "development-build-server": "BABEL_ENV=server-dev $(npm bin)/webpack --config ./webpack/webpack.config.server.development.babel.js --watch --colors --display-error-details",
    "development-server-delayed": "run-s delay development-server",
    "delay": "node ./src/sleep 10000",
    "development-server": "BABEL_ENV=server-dev nodemon --exec $(npm bin)/babel-node --watch ./src/server --watch ./src/pdf -- ./src/server/start.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/YannickDa/billify.git"
  },
  "keywords": [
    "bill"
  ],
  "author": "Yannick Dalbin <yannick@idvive.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/YannickDa/billify/issues"
  },
  "homepage": "https://github.com/YannickDa/billify#readme",
  "dependencies": {
    "html-pdf": "^2.1.0",
    "moment": "^2.17.1",
    "moment-timezone": "^0.5.11",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-moment": "^0.2.2"
  },
  "devDependencies": {
    "babel": "^6.23.0",
    "babel-cli": "^6.24.0",
    "babel-core": "^6.24.0",
    "babel-loader": "^6.4.1",
    "babel-preset-env": "^1.2.2",
    "babel-preset-react": "^6.23.0",
    "babel-preset-stage-0": "^6.22.0",
    "css-loader": "^0.27.3",
    "extract-text-webpack-plugin": "^2.1.0",
    "file-loader": "^0.10.1",
    "img-loader": "^2.0.0",
    "node-sass": "^4.5.1",
    "npm-run-all": "^4.0.2",
    "sass-loader": "^6.0.3",
    "style-loader": "^0.15.0",
    "universal-webpack": "^0.3.7",
    "webpack": "^2.2.1"
  }
}
